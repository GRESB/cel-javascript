{
  "name": "celjs",
  "version": "0.0.2",
  "description": "Parser and evaluator for Google's Common Expression Language (CEL) in JavaScript using ANTLR4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "int": "ts-node src/test.ts",
    "fetch-cel-spec": "ts-node scripts/fetchCelSpec.ts",
    "run-conformance-tests": "ts-node scripts/runConformanceTests.ts",
    "conformance": "npm run fetch-cel-spec && npm run run-conformance-tests"
  },
  "author": "yottanami",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/proto-loader": "^0.7.13",
    "antlr4": "^4.13.2",
    "antlr4ts": "^0.5.0-alpha.4",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "grpc": "^1.24.11",
    "path": "^0.12.7",
    "protobufjs": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.4.4",
    "vitest": "^0.0.111"
  }
}
